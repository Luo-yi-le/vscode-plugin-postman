{
  "name": "vscode-plugin-postman",
  "version": "0.1.1",
  "private": true,
  "description": "VSCode插件-postman",
  "keywords": [
    "postman",
    "post",
    "man",
    "vscode",
    "plugin"
  ],
  "publisher": "wulingshan",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./plugin/main.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Code插件demo",
      "properties": {
        "PostManPluginConfig.showIndex": {
          "type": "boolean",
          "default": true,
          "description": "启动时显示首页"
        }
      }
    },
    "commands": [{
      "command": "extension.postman.showindex",
      "title": "POSTMAN"
    }],
    "keybindings": [{
      "command": "extension.postman.showindex",
      "key": "ctrl+p",
      "mac": "cmd+p",
      "when": "editorTextFocus"
    }],
    "menus": {},
    "snippets": [],
    "viewsContainers": {
      "activitybar": [{
        "id": "home-icon",
        "title": "postman",
        "icon": "./plugin/image/icon.png"
      }]
    },
    "views": {
      "home-icon": [{
          "id": "index",
          "name": "记录"
        },
        {
          "id": "index1",
          "name": "记录1"
        }
      ]
    },
    "iconThemes": []
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.0",
    "axios": "^0.24.0",
    "clipboard": "^2.0.8",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.6",
    "lodash": "^4.17.21",
    "node-sass": "^4.14.1",
    "qs": "^6.10.2",
    "sass": "^1.45.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^4.5.4",
    "vue": "^2.6.11",
    "vue-axios": "^3.4.0",
    "vue-json-viewer": "^2.2.21",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11",
    "vscode": "^1.1.37"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}